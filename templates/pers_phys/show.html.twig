{% extends 'base.html.twig' %}

{% block title %}PersPhys{% endblock %}

{% block body %}
    <h1>PersPhys</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pers_phy.id }}</td>
            </tr>
            <tr>
                <th>Mat_pers</th>
                <td>{{ pers_phy.matPers }}</td>
            </tr>
            <tr>
                <th>Nom_pers</th>
                <td>{{ pers_phy.nomPers }}</td>
            </tr>
            <tr>
                <th>Pnom_pers</th>
                <td>{{ pers_phy.pnomPers }}</td>
            </tr>

    <a href="{{ path('app_pers_phys_index') }}" class="btn btn-primary" >Retour</a></br></br>

    <a href="{{ path('app_pers_phys_edit', {'id': pers_phy.id}) }}" class="btn btn-primary" >Editer</a></br></br>

    {{ include('pers_phys/_delete_form.html.twig') }}
